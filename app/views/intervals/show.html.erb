<div class="interval_show">

	<div class="grid_1 Edit">
		<%= link_to 'Edit', edit_interval_path(@interval) %> 
	</div>
	<div class="grid_15">
		<%= link_to 'Back', intervals_path %>
	</div>
	<div class="clear"></div>


	<div class="grid_12" id="sessTypePhrase">
		<div>
			<strong><%= @interval.session_type %></h1> <!--Session Type-->
